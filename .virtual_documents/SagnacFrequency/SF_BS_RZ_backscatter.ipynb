def __time_spans_to_nan(bs, time_spans):

    from numpy import where, nan

    for t1, t2 in time_spans:
        bs['fj_fs'] = where((bs.time1 > t1) & (bs.time1 < t2), nan, bs.fj_fs)
        bs['fj_bs'] = where((bs.time1 > t1) & (bs.time1 < t2), nan, bs.fj_bs)

    return bs


