def __df_to_stream(df0, dt, t1):

    st0 = Stream()

    for k in df.keys():

        _tr = Trace()
        _tr.data = np.array(df[k].values)
        _tr.stats.delta = dt
        _tr.stats.starttime = UTCDateTime(t1)
        _tr.stats.station = k

        st0 += _tr

    return st0


