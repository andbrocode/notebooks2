def __to_tilt(data):

    from numpy import pi, sqrt, arccos, deg2rad, arcsin
#     return -1 * (arccos((data * 632.8e-9 * 3) / ( (2*pi/86400) * 11.3 * sqrt(3) )) + deg2rad(48.162941))
    return (arcsin((data * 632.8e-9 * 3) / ( (2*pi/86400) * 11.3 * sqrt(3) )) - deg2rad(48.162941))


